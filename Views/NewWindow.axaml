<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Dock.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="350" d:DesignHeight="590"
        x:Class="Dock.Views.NewWindow"
        x:DataType="vm:NewWindowViewModel"
        Icon="/Assets/avalonia-logo.ico"
        Title="Avalonia_Dock_Menu"
        Width="350" Height="590">

	<Design.DataContext>
		<vm:MainWindowViewModel/>
	</Design.DataContext>

	<Grid>
		<StackPanel>
			<TextBox x:Name="NomeTextBox" />
			<TextBox x:Name="CaminhoTextBox"/>
			<TextBox x:Name="CaminhoIconeTextBox"/>
			<Button Content="Salvar" Click="Salvar_Click"/>
		</StackPanel>
	</Grid>
</Window>
